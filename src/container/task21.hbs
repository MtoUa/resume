<header class="card">
    {{#with heading}}
    <div class="card-body">
        <section class="card">
            <div class="card-body">
                {{#with main}}
                <h1>{{title}}</h1>
                <p>{{text}}</p>

                {{#with button_text}}
                <div>
                    <button class="btn btn-primary">
                        {{solid}}
                    </button>
                </div>
                <br />
                <div>
                    <button class="btn btn-outline-primary">
                        {{outline}}
                    </button>
                </div>
                {{/with}}
                {{/with}}
            </div>
        </section>

        <br />

        <section class="card">
            <div class="card-body">
                {{#with second}}
                <h2>{{title}}</h2>
                <p>{{text}}</p>

                {{#with button_text}}
                <div>
                    <button class="btn btn-secondary">
                        {{solid}}
                    </button>
                </div>
                <br />
                <div>
                    <button class="btn btn-outline-secondary">
                        {{outline}}
                    </button>
                </div>
                {{/with}}
                {{/with}}
            </div>
        </section>

        <br />

        <section class="card">
            <div class="card-body">
                {{#with subblock}}
                <h3>{{title}}</h3>
                <p>{{text}}</p>

                {{#with button_text}}
                <div>
                    <button class="btn btn-dark">
                        {{solid}}
                    </button>
                </div>
                <br />
                <div>
                    <button class="btn btn-outline-dark">
                        {{outline}}
                    </button>
                </div>
                {{/with}}
                {{/with}}
            </div>
        </section>
    </div>
    {{/with}}
</header>

<main class="card">
    {{#with nested_display}}
    <div class="card-body">

        <div class="display-1">{{title}}</div>

        <br />

        <section class="card">
            {{#with children_display}}
            <div class="card-body">
                <div class="display-4">{{title}}</div>
                <br />
                <p>{{text}}</p>
                <button class="btn btn-outline-warning">
                    {{button.text}}
                </button>
            </div>
            {{/with}}
        </section>

        <br />

        <h6>{{header_text}}</h6>
        <button class="btn btn-info">
            <span class="text-light">{{button.text}}</span>
        </button>
    </div>
    {{/with}}
</main>

<footer class="card">
    {{#with paragraph}}
    <div class="card-body">
        <p class="text-uppercase">{{title}}</p>
        <p class="text-lowercase">{{text}}</p>

        {{#with button}}
        <div>
            <a href="{{url}}" class="btn btn-link">
                {{text}}
            </a>
        </div>
        {{/with}}

        <br />

        <section class="card">
            <ul class="card-body">
                {{#each list_name}}
                <li>
                    <a href="{{url}}" class="btn btn-link">
                        {{text}}
                    </a>
                </li>
                {{/each}}
            </ul>
        </section>
    </div>
    {{/with}}
</footer>