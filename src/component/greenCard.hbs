<section class="card text-bg-success text-center">
    {{#with service}}
    <div class="card-body">
        <h2 class="card-title">{{title}}</h2>
        <p class="my-4">{{description}}</p>
        {{#each buttons}}
        <div class="mt-3">
            <a class="btn btn-primary" href="{{link}}">{{text}}</a>
        </div>
        {{/each}}
    </div>
    {{/with}}
</section>